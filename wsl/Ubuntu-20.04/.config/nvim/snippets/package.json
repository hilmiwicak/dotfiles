{
  "name": "snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "languages": [
    {
      "id": "all",
      "extensions": "*"
    }
  ],
  "contributes": {
    "snippets": [
      {
        "language": [
          "all"
        ],
        "path": "./global.json"
      },
      {
        "language": [
          "blade",
          "html"
        ],
        "path": "./html.json"
      },
      {
        "language": [
          "blade"
        ],
        "path": "./laravel.json"
      },
      {
        "language": [
          "go"
        ],
        "path": "./go.json"
      },
      {
        "language": [
          "css",
          "html"
        ],
        "path": "./css.json"
      },
      {
        "language": [
          "javascript",
          "javascriptreact",
          "html"
        ],
        "path": "./javascript.json"
      },
      {
        "language": [
          "markdown"
        ],
        "path": "./markdown.json"
      },
      {
        "language": [
          "bash",
          "shellscript",
          "shell",
          "sh",
          "zsh"
        ],
        "path": "./shell.json"
      }
    ]
  }
}
